version 41
__lua__
function _init()
	iplr()
	icrops()
	iinv()
end

function _update()
	uplr()
	ucrops()
	uinv()
end

function _draw()
	cls(11)
	map()
	dplr()
	dcrops()
	dinv()
end
-->8
--player
function iplr()
	plr={
		x=30,
		y=40,
		sp=plr_d
	}
end

function uplr()
--d-pad:

	--movement
	if btn(➡️) then
		plr.x+=1
		plr.sp=plr_r
	elseif btn(⬅️) then
		plr.x-=1
		plr.sp=plr_l
	elseif btn(⬆️) then
		plr.y-=1
		plr.sp=plr_u
	elseif btn(⬇️) then
		plr.y+=1
		plr.sp=plr_d
	end
	
--❎ button:

	--till/plant/water
	--harvest/item
	if btnp(❎) then
		--approx location of player
		ptx=flr((plr.x+7)/8)
		pty=flr((plr.y+15)/8)
		
		ptspr=mget(ptx,pty) --player tile sprite
		flag=fget(ptspr)
				
		--till
		if flag==f_empty then
		 mset(ptx,pty,tilled)
		end
		
		--plant
		if flag==f_tilled then
		 mset(ptx,pty,a_seed)
		 add(crops,{
		 	x=ptx,
		 	y=pty,
		 	water=false,
		 	t=0
		 })
		end
		
		--water
		if flag==f_seeds then
			--update map
			if ptspr==a_seed then
				mset(ptx,pty,a_water)
			end
			if ptspr==o_seed then
				mset(ptx,pty,o_water)
			end
			water(ptx,pty)
		end
		
		if flag==f_crop then
			harvest(ptx,pty)
		end
		
		if flag==f_item then
			take(ptx,pty)
		end
		
	end
end

function dplr()
	sspr(
		plr.sp.x,
		plr.sp.y,
		16,
		16,
		plr.x,
		plr.y
	)
end


-->8
--crops

function icrops()
	crops={}
end

function ucrops()
	for s in all(crops) do
		ms=mget(s.x,s.y)
		if s.water then
			s.t+=1
			if s.t==300 then
				if ms==a_water then
				 mset(s.x,s.y,a_crop)
				end
				if ms==o_water then
				 mset(s.x,s.y,o_crop)
				end
				s.water=false
			end
		end
	end
end

function dcrops()
end

function water(x,y)
	for v in all(crops) do
		if v.x==x and v.y==y then
			v.water=true
		end
	end
end
-->8
--inventory
function iinv()
	inv={}
end

function uinv()
end

function dinv()
	local i=0
	for k,v in pairs(inv) do
		local idx = 117-(11*i)
		local idy = 2
		sspr(0,8,10,10,idx,idy)
		spr(k,idx+1,idy+1)
		print(v,idx+1,idy+7,7)
		i+=1
	end
end

function harvest(x,y)
	take(x,y)
	harvest_cleanup(x,y)
end

function harvest_cleanup(x,y)
	for s in all(seeds) do
		if s.x==x and s.y==y then
			del(seeds,s)
		end
	end
end

-- pick up item
function take(x,y)
	s = mget(x,y)
	
	--special case for crops
	if (s==a_crop) s=apple
	if (s==o_crop) s=orange
	
	if inv[s]!=nil then
		inv[s]+=1
	else
		inv[s]=1
	end
	mset(x,y,0)
end

