--player
function iplr()
	plr={
		x=30,
		y=40,
		sp=plr_d
	}
end

function uplr()
--d-pad:

	--movement
	if show_sign then
		if (btnp(‚ùé)) toggle_sign()
		return
	end
	
	if btn(‚û°Ô∏è) then
		plr.x+=1
		plr.sp=plr_r
	elseif btn(‚¨ÖÔ∏è) then
		plr.x-=1
		plr.sp=plr_l
	elseif btn(‚¨ÜÔ∏è) then
		plr.y-=1
		plr.sp=plr_u
	elseif btn(‚¨áÔ∏è) then
		plr.y+=1
		plr.sp=plr_d
	end
	
--‚ùé button:

	--multi-modal input handler
	--‚ùé behavoir depends on player
	--sprite location on the map
	--and the state of the player's
	--inventory

	if btnp(‚ùé) then
		--approx location of player
		ptx=flr((plr.x+7)/8)
		pty=flr((plr.y+15)/8)
		
		ptspr=mget(ptx,pty) --player tile sprite
		flag=fget(ptspr)
		
		--read sign
		if flag==f_sign then
			toggle_sign()
		end
				
		--till
		if flag==f_empty then
		 mset(ptx,pty,tilled)
		end
		
		--plant
		if flag==f_tilled then
			if #inv > 0 then--has inv items
				sel=inv[selected]
				if (sel.s==a_seed_pk) seed=a_seed
				if (sel.s==o_seed_pk) seed=o_seed
				if seed!=nil and sel.amt>0 then
					plant(ptx,pty,seed)
				end
			end
		end
		
		--water
		if flag==f_seeds then
			--update map
			if ptspr==a_seed then
				mset(ptx,pty,a_water)
			end
			if ptspr==o_seed then
				mset(ptx,pty,o_water)
			end
			water(ptx,pty)
		end
		
		--harvest
		if flag==f_crop then
			harvest(ptx,pty)
		end
		if flag==f_item then
			take(ptx,pty)
		end
	end
	
	--üÖæÔ∏è button (z): cycle thru inv
end

function dplr()
	sspr(
		plr.sp.x,
		plr.sp.y,
		16,
		16,
		plr.x,
		plr.y
	)
end
