--crops

function icrops()
	crops={}
end

function ucrops()
	for s in all(crops) do
		ms=mget(s.x,s.y)
		if s.water and s.t<300 then
			s.t+=1
			if s.t==300 then
				if ms==a_water then
				 mset(s.x,s.y,a_crop)
				end
				if ms==o_water then
				 mset(s.x,s.y,o_crop)
				end
				s.water=false
			end
		end
	end
end

function dcrops()
end

function water(x,y)
	for v in all(crops) do
		if v.x==x and v.y==y then
			v.water=true
		end
	end
end

function plant(x,y,seed)
	inv[selected].amt-=1
	add(crops, {
		x=x,
		y=y,
		t=0,
		water=false
	})
	mset(ptx,pty,seed)
end
