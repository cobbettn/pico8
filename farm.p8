--inventory
function iinv()
	selected=nil
	inv={}
end

function uinv()
end

function dinv()
	local i=0
	for item in all(inv) do
		if item.amt > 0 then
			local idx = 117-(11*i)
			local idy = 2
			if selected==i+1 then
				sspr(56,8,10,10,idx,idy)
			end
			sspr(0,8,10,10,idx,idy)
			spr(item.s,idx+1,idy+1)
			print(item.amt,idx+6,idy+11,7)
			i+=1
		else
			--cleanup
			deli(inv,i+1)
			if #inv>0 then
				if inv[selected]==nil then
					selected=1
				end				
			else
				selected=nil
			end
		end
	end
end

function harvest(x,y)
	take(x,y)
	for s in all(seeds) do
		if s.x==x and s.y==y then
			del(seeds,s)
		end
	end
end

-- pick up item
function take(x,y)
	s = mget(x,y)
	
	--for unharvested crops
	if (s==a_crop) s=apple
	if (s==o_crop) s=orange
		
	if (selected==nil) selected=1--1-indexed tables

--	randomize 0,1,2
	mset(x,y,0) 
	
	add_inv(s)
end

function add_inv(s)
	found = false
	for i in all(inv) do
		if i.s==s then
			found = true
			i.amt+=1
			break
		end
	end
	if not(found) then
		amt=1
		if s==a_seed_pk or s==o_seed_pk then
			amt=9
		end
		add(inv,{
			s=s,
			amt=amt
		})
	end
end

function s_in_inv(s)
	for i in all(inv) do
		if (i.s==s) return true
	end
	return false
end
